---
import DownloadRedirectLayout from "../../../layouts/DownloadRedirectLayout.astro";
import { pizzametrics as app } from "../../../data/apps/pizzametrics";

export const prerender = true;
---

<DownloadRedirectLayout
    title={`Descargar ${app.title}`}
    appleStoreUrl={app.stores.appStore}
    googlePlayUrl={app.stores.googlePlay}
    fallbackUrl={`/apps/${app.slug}/`}
/>
