---
import DownloadRedirectLayout from "../../../layouts/DownloadRedirectLayout.astro";
import { lexiCrash as app } from "../../../data/apps/lexi-crash";

export const prerender = true;

const title = `Descargar ${app.title}`;
const appleStoreUrl = app.stores.appStore;
const googlePlayUrl = app.stores.googlePlay;
const fallbackUrl = `/apps/${app.slug}/`;
---

<DownloadRedirectLayout
    title={title}
    appleStoreUrl={appleStoreUrl}
    googlePlayUrl={googlePlayUrl}
    fallbackUrl={fallbackUrl}
/>
